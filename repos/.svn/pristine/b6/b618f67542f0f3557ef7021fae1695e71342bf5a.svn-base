    \    Log    ${i}:${组名}
    \    Sleep    1
    \    Wait Until Element Is Enabled    xpath=//*[@id="integralTable"]/tbody/tr[${i}]/td/button[@ng-bind="i.memberGroup.name" and text()="${组名}"]
    \    Sleep    1
    Sleep    3
